# Production environment variables
# Note: NODE_ENV is handled by Vite automatically
MODE=production

# Azure Speech Services (Production)
# Replace with your production keys
VITE_AZURE_SPEECH_KEY=your_production_speech_key_here
VITE_AZURE_SPEECH_REGION=your_production_region_here
VITE_AZURE_SPEECH_VOICE=en-US-JennyNeural

# LLM Configuration (Production)
# Replace with your production keys
VITE_OPENAI_API_KEY=your_production_openai_key_here
VITE_LLM_MODEL=gpt-3.5-turbo
VITE_LLM_MAX_TOKENS=150
VITE_LLM_TEMPERATURE=0.7

# Performance optimizations
VITE_BUNDLE_ANALYZER=false
VITE_DEV_TOOLS=false