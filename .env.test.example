# Environment variables for testing
# Copy this to .env.test and fill in your actual values

# Azure Speech Services
VITE_AZURE_SPEECH_KEY=your_speech_key_here
VITE_AZURE_SPEECH_REGION=your_region_here
VITE_AZURE_SPEECH_VOICE=en-US-JennyNeural

# Azure OpenAI
VITE_AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
VITE_AZURE_OPENAI_KEY=your_openai_key_here
VITE_AZURE_OPENAI_DEPLOYMENT=your_deployment_name

# Test Configuration
PLAYWRIGHT_TIMEOUT=30000
PLAYWRIGHT_HEADLESS=true
