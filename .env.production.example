# Production Environment Variables

# Azure Speech Services (Required for TTS)
VITE_AZURE_SPEECH_KEY=your_production_key_here
VITE_AZURE_SPEECH_REGION=eastus
VITE_AZURE_SPEECH_VOICE=en-US-JennyNeural

# Azure OpenAI (Required for LLM Chat)
VITE_AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
VITE_AZURE_OPENAI_KEY=your_production_openai_key
VITE_AZURE_OPENAI_DEPLOYMENT=your_deployment_name
VITE_AZURE_OPENAI_API_VERSION=2024-05-01-preview
